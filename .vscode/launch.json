    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "env": {
                "MXNET_CUDNN_AUTOTUNE_DEFAULT": "0"
            },
            "program": "${workspaceFolder}/main.py",
            "args": [
                "--batch_size=2",
                "--backend=mxnet",
                "--model_idx=7",
                "--debug",
                "--num_warmup=10",
                "--input_channels=1",
                "--input_dim=64"
            ],
            "console": "integratedTerminal"
        }
    ]
}